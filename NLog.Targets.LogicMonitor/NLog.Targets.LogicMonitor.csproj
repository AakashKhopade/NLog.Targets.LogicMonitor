<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>net6.0</TargetFramework>
    <ImplicitUsings>enable</ImplicitUsings>
    <Nullable>enable</Nullable>
    <PackageId>NLogs.Targets.LogicMonitor</PackageId>
    <Version>0.1.2-alpha</Version>
    <RepositoryUrl>https://github.com/AakashKhopade/serilog-sinks-logicmonitor</RepositoryUrl>
    <RepositoryType>git</RepositoryType>
    <PackageReleaseNotes>Logs generated by NLogs are ingested directly to LogicMonitor.</PackageReleaseNotes>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="NLog" Version="5.0.5" />
    <PackageReference Include="Logicmonitor.DataSDK" Version="0.1.2-alpha" />
  </ItemGroup>
</Project>
